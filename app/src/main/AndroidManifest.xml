<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bottombar.sample">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:launchMode="singleInstance">

        </activity>

        <activity android:name=".SearchOrderActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".OrderListActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value=".SearchOrderActivity"
                      ></meta-data>
        </activity>
        <activity android:name=".OrderMenuActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value=".OrderListActivity"

                ></meta-data>
        </activity>

        <activity android:name=".OrderBasicInfoActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value=".OrderMenuActivity"></meta-data>
        </activity>

        <activity android:name=".OrderShougouActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value=".OrderMenuActivity"></meta-data>
        </activity>

        <activity android:name=".OrderChuyunActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value=".OrderMenuActivity"></meta-data>
        </activity>

        <activity android:name=".OrderFukuangActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value=".OrderMenuActivity"></meta-data>
        </activity>

        <activity android:name=".OrderShouhuiActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value=".OrderMenuActivity"></meta-data>
        </activity>
    </application>

</manifest>